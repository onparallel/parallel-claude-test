{
  "name": "@parallel/client",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf '{out,.next}'",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "generate-graphql-types": "graphql-codegen",
    "extract-i18n-terms": "yarn workspace @parallel/bin extract-i18n-terms --input '../client/{components,pages,utils}/**/*.tsx' --output '../client/lang'",
    "generate-i18n-files": "yarn workspace @parallel/bin generate-i18n-files --input '../client/lang' --output-raw '../client/public/static/lang' --output-compiled '../client/public/static/lang/compiled'",
    "build-static-prod": "yarn clean && ENV=production next build && ENV=production next export",
    "deploy-static-landing-prod": "aws s3 sync out s3://parallel-static-landing-prod --profile parallel-deploy && aws cloudfront create-invalidation --distribution-id E200FE05XP92NJ --paths \"/*\" --profile parallel-deploy",
    "build-static-staging": "yarn clean && ENV=staging next build && ENV=staging next export && echo 'Disallow: /' >> out/robots.txt",
    "deploy-static-landing-staging": "aws s3 sync out s3://parallel-static-landing-staging --profile parallel-deploy && aws cloudfront create-invalidation --distribution-id E2JVYY22XTRJ1B --paths \"/*\" --profile parallel-deploy"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@chakra-ui/core": "^0.6.1",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@formatjs/intl-utils": "^2.2.0",
    "@next/bundle-analyzer": "^9.3.0",
    "@welldone-software/why-did-you-render": "^4.0.5",
    "apollo-boost": "^0.4.7",
    "apollo-link-context": "^1.0.19",
    "axios": "^0.19.2",
    "beautiful-react-hooks": "^0.23.1",
    "clipboard-copy": "^3.1.0",
    "dnd-core": "^10.0.2",
    "emotion-theming": "^10.0.27",
    "graphql": "^14.6.0",
    "markdown-to-jsx": "^6.11.0",
    "next": "^9.3.1",
    "node-fetch": "^2.6.0",
    "react": "^16.13.1",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.13.1",
    "react-dropzone": "^10.2.1",
    "react-hook-form": "^5.1.3",
    "react-intl": "^4.2.2",
    "react-select": "^3.1.0",
    "remeda": "^0.0.17",
    "slate": "^0.57.1",
    "slate-history": "^0.57.1",
    "slate-react": "^0.57.1",
    "smooth-scroll-into-view-if-needed": "^1.1.27"
  },
  "devDependencies": {
    "@formatjs/cli": "^1.3.1",
    "@graphql-codegen/cli": "^1.13.1",
    "@graphql-codegen/typescript": "^1.13.1",
    "@graphql-codegen/typescript-operations": "^1.13.1",
    "@types/async": "^3.0.8",
    "@types/graphql": "^14.5.0",
    "@types/markdown-to-jsx": "^6.11.0",
    "@types/node": "^13.9.3",
    "@types/node-fetch": "^2.5.5",
    "@types/react": "^16.9.25",
    "@types/react-select": "^3.0.11",
    "babel-plugin-emotion": "^10.0.29",
    "chalk": "^3.0.0",
    "intl-messageformat-parser": "^4.1.1",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  }
}
