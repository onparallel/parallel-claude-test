  "scripts": {
    "clean": "rimraf '{out,.next}'",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "prebuild": "yarn generate-i18n-files",
    "postbuild": "yarn _strip-js --input ../client/.next/server/pages/es/update.html && yarn _strip-js --input ../client/.next/server/pages/en/update.html",
    "generate-graphql-types": "graphql-codegen",
    "extract-i18n-terms": "yarn concurrently \"yarn:extract-i18n-terms:* start\"",
    "extract-i18n-terms:recipient": "yarn workspace @parallel/bin extract-i18n-terms --cwd '../client' --locales en es ca it pt --input './pages/{pp,petition}/**/*.{ts,tsx}' './pages/{maintenance,thanks,update,404}.tsx' --output './lang/recipient' --exclude-pragma no-recipient",
    "extract-i18n-terms:app": "yarn workspace @parallel/bin extract-i18n-terms --cwd '../client' --locales en es --input './pages/**/*.{ts,tsx}' --output './lang'",
    "generate-i18n-files": "yarn concurrently \"yarn:generate-i18n-files:* start\"",
    "generate-i18n-files:recipient": "yarn workspace @parallel/bin generate-i18n-files --locales en es ca it pt --input '../client/lang/recipient' --pick-missing-from '../client/lang' --output-raw '../client/public/static/lang/recipient' --output-compiled '../client/public/static/lang/recipient/compiled'",
    "generate-i18n-files:app": "yarn workspace @parallel/bin generate-i18n-files --locales en es --input '../client/lang' --output-raw '../client/public/static/lang' --output-compiled '../client/public/static/lang/compiled'",
    "download-flag-emojis": "yarn workspace @parallel/bin download-flag-emojis --output '../client/public/static/countries/flags'",
    "download-countries-i18n": "yarn workspace @parallel/bin download-cldr-data --dataset country-names --locales en es ca it pt --output '../client/public/static/countries/countries_#LOCALE#.json'",
    "download-timezone-list": "yarn workspace @parallel/bin download-timezone-list --output '../client/utils/timezones.json'",
    "why-recipient": "yarn workspace @parallel/bin node dist/why-included.js --cwd ../client --from './pages/{pp,petition}/**/*.{ts,tsx}' './pages/{maintenance,thanks,update,404}.tsx' --to",
    "storybook": "storybook dev --port 6006",
    "_strip-js": "yarn workspace @parallel/bin strip-js"
  },
