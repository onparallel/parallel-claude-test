{
  "name": "@parallel/client",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf '{out,.next}'",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "generate-graphql-types": "graphql-codegen",
    "extract-i18n-terms": "yarn workspace @parallel/bin extract-i18n-terms --locales en es --input '../client/{components,pages,utils}/**/*.tsx' --output '../client/lang'",
    "generate-i18n-files": "yarn workspace @parallel/bin generate-i18n-files --locales en es --input '../client/lang' --output-raw '../client/public/static/lang' --output-compiled '../client/public/static/lang/compiled'",
    "build-production": "yarn clean && cross-env ENV=production next build && cross-env ENV=production next export",
    "deploy-static-landing-prod": "aws s3 sync out s3://parallel-static-landing-prod --profile parallel-deploy && aws cloudfront create-invalidation --distribution-id E200FE05XP92NJ --paths \"/*\" --profile parallel-deploy",
    "build-staging": "yarn clean && cross-env ENV=staging next build && cross-env ENV=production next export && echo Disallow: / >> out/robots.txt",
    "deploy-static-landing-staging": "aws s3 sync out s3://parallel-static-landing-staging --profile parallel-deploy && aws cloudfront create-invalidation --distribution-id E2JVYY22XTRJ1B --paths \"/*\" --profile parallel-deploy"
  },
  "dependencies": {
    "@apollo/react-common": "^3.1.4",
    "@apollo/react-hooks": "^3.1.5",
    "@apollo/react-ssr": "^3.1.5",
    "@chakra-ui/core": "^0.7.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@formatjs/intl-utils": "^2.2.4",
    "@graphql-codegen/cli": "^1.13.3",
    "@graphql-codegen/typescript-react-apollo": "^1.13.3",
    "@next/bundle-analyzer": "^9.3.6",
    "@reach/auto-id": "^0.10.1",
    "@types/js-cookie": "^2.2.6",
    "@welldone-software/why-did-you-render": "^4.1.3",
    "apollo-boost": "^0.4.7",
    "apollo-link-context": "^1.0.20",
    "axios": "^0.19.2",
    "beautiful-react-hooks": "^0.23.1",
    "clipboard-copy": "^3.1.0",
    "date-fns": "^2.12.0",
    "dnd-core": "^10.0.2",
    "emotion-theming": "^10.0.27",
    "graphql": "^15.0.0",
    "js-cookie": "^2.2.1",
    "markdown-to-jsx": "^6.11.1",
    "next": "^9.3.6",
    "node-fetch": "^2.6.0",
    "react": "^16.13.1",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-hook-form": "^5.6.0",
    "react-intl": "^4.5.1",
    "react-select": "^3.1.0",
    "remeda": "^0.0.19",
    "slate": "^0.57.2",
    "slate-history": "^0.57.2",
    "slate-react": "^0.57.2",
    "smooth-scroll-into-view-if-needed": "^1.1.27",
    "vanilla-text-mask": "^5.1.1"
  },
  "devDependencies": {
    "@formatjs/cli": "^1.6.9",
    "@graphql-codegen/cli": "^1.13.3",
    "@graphql-codegen/typescript": "^1.13.3",
    "@graphql-codegen/typescript-operations": "^1.13.3",
    "@types/async": "^3.2.1",
    "@types/graphql": "^14.5.0",
    "@types/markdown-to-jsx": "^6.11.0",
    "@types/node": "^13.13.4",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.34",
    "@types/react-select": "^3.0.12",
    "babel-plugin-emotion": "^10.0.33",
    "chalk": "^4.0.0",
    "cross-env": "^7.0.2",
    "intl-messageformat-parser": "^5.0.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "svgo": "^1.3.2",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  }
}
