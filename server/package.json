{
  "name": "@parallel/server",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --ignore-watch \"(graphql/__types.ts|src/(emails|pdf))\" --no-notify --transpile-only --respawn ./src",
    "debug": "cross-env NODE_ENV=development ts-node-dev --inspect --ignore-watch \"(graphql/__types.ts|src/(emails|pdf))\" --no-notify --transpile-only --respawn ./src",
    "build": "tsc --project tsconfig.prod.json",
    "prebuild": "yarn generate-i18n-files",
    "migrate": "knex migrate:latest",
    "postmigrate": "yarn generate-db-types",
    "migrate-make": "knex migrate:make --client pg",
    "migrate-dryrun": "knex migrate:list",
    "migrate-down": "knex migrate:down",
    "postmigrate-down": "yarn generate-db-types",
    "seed": "knex seed:run",
    "generate-db-types": "ts-node ./bin/generate-db-types.ts",
    "test": "jest --runInBand",
    "extract-i18n-terms": "yarn workspace @parallel/bin extract-i18n-terms --locales en es --input '../server/src/**/*.{ts,tsx}' --output '../server/lang'",
    "generate-i18n-files": "yarn workspace @parallel/bin generate-i18n-files --locales en es --input '../server/lang' --output-compiled '../server/lang/compiled'",
    "download-pdf-fonts": "yarn workspace @parallel/bin download-fonts --output '../client/public/static/fonts/pdf'",
    "dev-workers": "yarn concurrently \"yarn:worker:* start\"",
    "worker:email-events": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/email-events.ts",
    "worker:email-sender": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/email-sender.ts",
    "worker:event-processor": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/event-processor.ts",
    "worker:reminder-trigger": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/reminder-trigger.ts",
    "worker:scheduled-trigger": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/scheduled-trigger.ts",
    "worker:signature": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/signature-worker.ts",
    "worker:tasks": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/task-worker.ts",
    "worker:delay-queue": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/delay-queue.ts",
    "worker:petition-notifications": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/petition-notifications.ts",
    "worker:organization-limits": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/organization-limits.ts",
    "worker:anonymizer": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/anonymizer.ts",
    "worker:old-notifications": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/old-notifications.ts",
    "localtunnel": "ts-node ./bin/launch-localtunnel.ts",
    "storybook": "concurrently \"yarn _stories\" \"yarn _storybook\"",
    "_stories": "SERVER_STORIES_PORT=5000 ts-node-dev --watch ./stories.tsx --ignore-watch ./src --files ./stories.tsx --transpile-only --respawn",
    "_storybook": "SERVER_STORIES_PORT=5000 NODE_OPTIONS=\"--preserve-symlinks --preserve-symlinks-main\" storybook dev --port 60000 --quiet",
    "postinstall": "cd .. && yarn patch-package",
    "generate-graphql-types": "graphql-codegen",
    "run-ts-script": "ts-node --project tsconfig.json --transpile-only",
    "flush-sqs": "docker-compose -f ../ops/dev/docker-compose.yml restart queue",
    "archive-migrations": "ts-node ./migrations/scripts/archive-migrations.ts",
    "sync-migrations": "ts-node ./migrations/scripts/sync-migrations.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.3.2",
