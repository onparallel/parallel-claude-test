    "@types/express": "^4.17.2",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql-iso-date": "^3.3.3",
    "@types/jest": "^25.1.2",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^13.7.0",
