    "graphql-upload": "^12.0.0",
    "inversify": "^5.1.1",
    "json-schema-to-ts": "^1.6.4",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.11",
    "mime-match": "^1.0.2",
