    "pg": "^8.7.1",
    "playwright": "^1.15.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.20.12",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "remeda": "^0.0.30",
    "request-ip": "^2.1.3",
    "sanitize-filename": "^1.6.3",
    "signaturit-sdk": "^1.2.1",
    "sqs-consumer": "^5.6.0",
    "timezone-support": "^2.0.2",
    "ts-node": "^10.2.1",
    "typed-url-params": "^1.0.1",
    "typescript-memoize": "^1.0.1",
    "ua-parser-js": "^0.7.28",
    "winston": "^3.3.3",
    "winston-cloudwatch": "^3.0.2",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/typescript": "^2.2.2",
    "@graphql-codegen/typescript-operations": "^2.1.4",
    "@graphql-tools/graphql-tag-pluck": "7.1.1",
    "@storybook/addon-a11y": "^6.3.8",
    "@storybook/addon-controls": "^6.3.8",
    "@storybook/addon-docs": "^6.3.8",
    "@storybook/addon-toolbars": "^6.3.8",
    "@storybook/server": "^6.3.8",
    "@types/analytics-node": "^3.1.6",
    "@types/archiver": "^5.1.1",
    "@types/content-disposition": "^0.5.4",
    "@types/cookie": "^0.4.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/cron": "^1.7.3",
    "@types/escape-html": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/fold-to-ascii": "^5.0.0",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/graphql-type-json": "^0.3.2",
