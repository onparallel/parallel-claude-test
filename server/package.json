{
  "name": "@parallel/server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --ignore-watch graphql/__types.ts --no-notify --transpile-only --respawn ./src",
    "debug": "ts-node-dev --inspect --ignore-watch graphql/__types.ts --no-notify --transpile-only --respawn ./src",
    "nodemon": "nodemon --watch 'src/' --ignore 'src/graphql/__types.ts' --exec 'node --inspect -r ts-node/register/transpile-only' src/index.ts",
    "build": "tsc --project tsconfig.prod.json",
    "migrate": "knex migrate:latest",
    "postmigrate": "yarn generate-db-types",
    "migrate-make": "knex migrate:make",
    "migrate-dryrun": "knex migrate:list",
    "migrate-down": "knex migrate:down",
    "postmigrate-down": "yarn generate-db-types",
    "seed": "knex seed:run",
    "generate-db-types": "ts-node ./bin/generate-db-types.ts",
    "test": "jest --runInBand",
    "emails": "ts-node-dev --watch ./emails.tsx --ignore-watch ./src --files ./emails.tsx",
    "extract-i18n-terms": "yarn workspace @parallel/bin extract-i18n-terms --locales en es --input '../server/src/emails/{components,common}/**/*.{ts,tsx}' --output '../server/lang'",
    "generate-i18n-files": "yarn workspace @parallel/bin generate-i18n-files --locales en es --input '../server/lang' --output-compiled '../server/lang/compiled'",
    "dev-workers": "yarn concurrently \"yarn:worker:* start\"",
    "worker:email-events": "yarn ts-node-dev --project tsconfig.worker.json src/workers/email-events.ts",
    "worker:email-sender": "yarn ts-node-dev --project tsconfig.worker.json src/workers/email-sender.ts",
    "worker:reminder-trigger": "yarn ts-node-dev --project tsconfig.worker.json src/workers/reminder-trigger.ts",
    "worker:scheduled-trigger": "yarn ts-node-dev --project tsconfig.worker.json src/workers/scheduled-trigger.ts"
  },
  "dependencies": {
    "@nexus/schema": "^0.16.0",
    "amazon-cognito-identity-js": "^4.5.0",
    "apollo-server-express": "^2.18.2",
    "apollo-server-plugin-base": "^0.10.1",
    "archiver": "^5.0.2",
    "async": "^3.2.0",
    "aws-sdk": "^2.773.0",
    "body-parser": "^1.19.0",
    "content-disposition": "^0.5.3",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dataloader": "^2.0.0",
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.0.12",
    "dotenv": "^8.2.0",
    "escape-string-regexp": "^4.0.0",
    "exceljs": "^4.1.1",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "fast-safe-stringify": "^2.0.7",
    "graphql": "^15.3.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-type-json": "^0.3.2",
    "inversify": "^5.0.1",
    "jwt-decode": "^3.0.0",
    "knex": "^0.21.6",
    "mjml": "^4.7.1",
    "mjml-react": "^1.0.58",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.14",
    "outdent": "^0.7.1",
    "pg": "^8.4.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-intl": "^5.8.6",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "remeda": "^0.0.23",
    "sanitize-filename": "^1.6.3",
    "sqs-consumer": "^5.4.0",
    "timezone-support": "^2.0.2",
    "uuid": "^8.3.1",
    "winston": "^3.3.3",
    "winston-cloudwatch": "^2.3.2",
    "yargs": "^16.1.0"
  },
  "devDependencies": {
    "@types/archiver": "^3.1.1",
    "@types/async": "^3.2.3",
    "@types/body-parser": "^1.19.0",
    "@types/content-disposition": "^0.5.3",
    "@types/cookie": "^0.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.8",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.8",
    "@types/express-graphql": "^0.9.0",
    "@types/faker": "^5.1.2",
    "@types/graphql": "^14.5.0",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest": "^26.0.14",
    "@types/jwt-decode": "^2.2.1",
    "@types/mjml": "^4.0.4",
    "@types/mjml-react": "^1.0.4",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.11.10",
    "@types/node-fetch": "^2.5.7",
    "@types/nodemailer": "^6.4.0",
    "@types/prettier": "^2.1.5",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/redis": "^2.8.28",
    "@types/uuid": "^8.3.0",
    "@types/yargs": "^15.0.9",
    "apollo-server-testing": "^2.18.2",
    "camelcase": "^6.1.0",
    "concurrently": "^5.3.0",
    "faker": "^5.1.0",
    "jest": "^26.6.0",
    "graphql-tag": "^2.11.0",
    "livereload": "^0.9.1",
    "nodemon": "^2.0.5",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.3"
  }
}
