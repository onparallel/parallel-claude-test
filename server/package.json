{
  "name": "@parallel/server",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --ignore-watch \"(graphql/__types.ts|src/(emails|pdf))\" --no-notify --transpile-only --respawn ./src",
    "debug": "cross-env NODE_ENV=development DEBUG=true ts-node-dev --inspect --ignore-watch \"(graphql/__types.ts|src/(emails|pdf))\" --no-notify --transpile-only --respawn ./src",
    "build": "tsc --project tsconfig.prod.json",
    "start": "cross-env NODE_ENV=development node dist/index.js",
    "prebuild": "yarn generate-i18n-files",
    "migrate": "knex migrate:latest",
    "postmigrate": "yarn generate-db-types",
    "migrate-make": "knex migrate:make --client pg",
    "migrate-dryrun": "knex migrate:list",
    "migrate-down": "knex migrate:down",
    "postmigrate-down": "yarn generate-db-types",
    "seed": "cross-env NODE_ENV=development knex seed:run --specific=dev.ts",
    "seed:standard-list-definitions": "cross-env NODE_ENV=development knex seed:run --specific=standard-list-definitions.ts",
    "seed:mock-dashboard": "cross-env NODE_ENV=development knex seed:run --specific=mock-dashboard.ts",
    "generate-db-types": "ts-node ./bin/generate-db-types.ts",
    "test": "jest --runInBand",
    "extract-i18n-terms": "yarn concurrently \"yarn:extract-i18n-terms:* start\"",
    "extract-i18n-terms:app": "yarn workspace @parallel/bin extract-i18n-terms --cwd '../server' --locales en es --input '../server/src/**/*.{ts,tsx}' --output '../server/lang'",
    "extract-i18n-terms:recipient": "yarn workspace @parallel/bin extract-i18n-terms --cwd '../server' --locales ca it pt --input '../server/src/emails/emails/recipient/**/*.{ts,tsx}' '../server/src/pdf/documents/recipient/**/*.{ts,tsx}' '../server/lang/recipient-additional-terms.ts' --output '../server/lang'",
    "generate-i18n-files": "yarn concurrently \"yarn:generate-i18n-files:* start\"",
    "generate-i18n-files:app": "yarn workspace @parallel/bin generate-i18n-files --locales en es --input '../server/lang' --output-compiled '../server/lang/compiled'",
    "generate-i18n-files:recipient": "yarn workspace @parallel/bin generate-i18n-files --locales ca it pt --input '../server/lang' --output-compiled '../server/lang/compiled'",
    "download-countries-i18n": "yarn workspace @parallel/bin download-cldr-data --dataset country-names --locales en es ca it pt --output '../server/data/countries/countries_#LOCALE#.json'",
    "download-currencies-i18n": "yarn workspace @parallel/bin download-cldr-data --dataset currency-names --locales en es ca it pt --output '../server/data/currencies/currencies_#LOCALE#.json'",
    "download-currencies-unit": "yarn workspace @parallel/bin download-cldr-data --dataset currency-basic-unit --locales en --output '../server/data/currencies/units.json'",
    "download-document-fonts": "yarn workspace @parallel/bin download-fonts --output '../client/public/static/fonts/pdf' --output-manifest '../client/utils/fonts.json' '../server/src/pdf/utils/fonts.json' --fonts  'IBM Plex Sans' 'Roboto Slab' 'Merriweather' 'Playfair Display' 'Lora' 'PT Serif' 'Source Serif 4@Source Serif Pro' 'IBM Plex Serif' 'Cormorant Garamond' 'Alegreya' 'Tinos' 'Libre Baskerville' 'Noto Serif' 'Roboto' 'Open Sans' 'Lato' 'Montserrat' 'Poppins' 'Source Sans 3@Source Sans Pro' 'Noto Sans' 'Raleway' 'Nunito' 'Rubik' 'Parisienne' 'Rubik Wet Paint' 'Inconsolata'",
    "download-international-fonts": "yarn workspace @parallel/bin download-fonts --output '../client/public/static/fonts/pdf' --output-manifest '../server/src/pdf/utils/international_fonts.json' --fonts 'Noto Sans '{Myanmar,Arabic,Hebrew,Georgian,SC,KR,Devanagari,Armenian,Khmer,JP,Kannada,'Canadian Aboriginal',Ethiopic,Bengali,Thai,Tamil,Lao,Sinhala,'Ol Chiki',Tifinagh,Yi,Syriac,Thaana,Vai,Cherokee,'Tai Tham','Tai Viet',Javanese,Telugu,Gujarati,Malayalam,Gurmukhi} 'Noto Serif Tibetan@Noto Sans Tibetan' 'Noto Emoji'",
    "dev-workers": "yarn concurrently \"yarn:worker:* start\"",
    "worker:email-events": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/email-events.ts",
    "worker:email-sender": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/email-sender.ts",
    "worker:event-processor": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/event-processor.ts",
    "worker:reminder-trigger": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/reminder-trigger.ts",
    "worker:scheduled-trigger": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/scheduled-trigger.ts",
    "worker:signature": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/signature-worker.ts",
    "worker:webhooks": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/webhooks-worker.ts",
    "worker:tasks": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/task-worker.ts",
    "worker:delay-queue": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/delay-queue.ts",
    "worker:petition-notifications": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/petition-notifications.ts",
    "worker:organization-limits": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/organization-limits.ts",
    "worker:anonymizer": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/anonymizer.ts",
    "worker:old-notifications": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/old-notifications.ts",
    "worker:expiring-properties": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/expiring-properties.ts",
    "worker:background-check-monitor": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/background-check-monitor.ts",
    "worker:adverse-media-monitor": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/adverse-media-monitor.ts",
    "localtunnel": "ts-node ./bin/launch-localtunnel.ts",
    "storybook": "concurrently \"yarn storybook:server\" \"yarn storybook:client\"",
    "storybook:server": "SERVER_STORIES_PORT=5001 ts-node-dev --watch ./stories.tsx --ignore-watch ./src --files ./stories.tsx --transpile-only --respawn",
    "storybook:client": "SERVER_STORIES_PORT=5001 storybook dev --port 60000 --quiet",
    "postinstall": "cd .. && yarn patch-package",
    "generate-graphql-types": "graphql-codegen",
    "flush-sqs": "docker-compose -f ../ops/dev/docker-compose.yml restart queue",
    "archive-migrations": "ts-node ./migrations/scripts/archive-migrations.ts",
    "sync-migrations": "ts-node ./migrations/scripts/sync-migrations.ts",
    "profile-types:remove-unused": "node dist/one-offs/remove-unused-profile-types.js",
    "profile-types:sync-standard-types": "node dist/one-offs/sync-standard-profile-types.js",
    "profile-types:reorder-properties": "node dist/one-offs/reorder-profile-type-properties.js",
    "why-recipient": "yarn workspace @parallel/bin node dist/why-included.js --cwd ../server --from '../server/src/emails/emails/recipient/**/*.{ts,tsx}' '../server/src/pdf/documents/recipient/**/*.{ts,tsx}' '../server/lang/recipient-additional-terms.ts' --to"
  },
