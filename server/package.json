{
  "name": "@parallel/server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --inspect --ignore-watch graphql/__types.ts --no-notify --transpileOnly --respawn ./src",
    "build": "tsc --project tsconfig.prod.json",
    "migrate": "knex migrate:latest",
    "postmigrate": "yarn generate-db-types",
    "migrate-make": "knex migrate:make",
    "migrate-dryrun": "knex migrate:list",
    "migrate-down": "knex migrate:down",
    "postmigrate-down": "yarn generate-db-types",
    "seed": "knex seed:run",
    "generate-db-types": "ts-node ./bin/generate-db-types.ts",
    "test": "jest --runInBand",
    "emails": "ts-node-dev --watch ./emails.tsx --ignore-watch ./src --files ./emails.tsx",
    "extract-i18n-terms": "yarn workspace @parallel/bin extract-i18n-terms --input '../server/src/emails/{components,common}/**/*.{ts,tsx}' --output '../server/src/emails/lang'",
    "generate-i18n-files": "yarn workspace @parallel/bin generate-i18n-files --input '../server/src/emails/lang' --output-compiled '../server/src/emails/lang/compiled'"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^4.2.1",
    "apollo-server-express": "^2.12.0",
    "async": "^3.2.0",
    "aws-sdk": "^2.656.0",
    "body-parser": "^1.19.0",
    "content-disposition": "^0.5.3",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dataloader": "^2.0.0",
    "date-fns": "^2.12.0",
    "date-fns-tz": "^1.0.10",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "fast-safe-stringify": "^2.0.7",
    "graphql": "^15.0.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-type-json": "^0.3.1",
    "inversify": "^5.0.1",
    "jwt-decode": "^2.2.0",
    "knex": "^0.20.14",
    "mjml": "^4.6.2",
    "mjml-react": "^1.0.57",
    "morgan": "^1.10.0",
    "nexus": "^0.12.0",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.6",
    "outdent": "^0.7.1",
    "pg": "^8.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^4.4.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "remeda": "^0.0.19",
    "sqs-consumer": "^5.4.0",
    "timezone-support": "^2.0.2",
    "uuid": "^7.0.3",
    "winston": "^3.2.1",
    "winston-cloudwatch": "^2.3.2"
  },
  "devDependencies": {
    "@types/async": "^3.2.0",
    "@types/body-parser": "^1.19.0",
    "@types/content-disposition": "^0.5.3",
    "@types/cookie": "^0.3.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.6",
    "@types/express-graphql": "^0.9.0",
    "@types/faker": "^4.1.11",
    "@types/graphql-iso-date": "^3.3.3",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest": "^25.2.1",
    "@types/jwt-decode": "^2.2.1",
    "@types/mjml": "^4.0.4",
    "@types/mjml-react": "^1.0.3",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.11.1",
    "@types/node-fetch": "^2.5.6",
    "@types/nodemailer": "^6.4.0",
    "@types/prettier": "^2.0.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/redis": "^2.8.17",
    "@types/uuid": "^7.0.2",
    "camelcase": "^6.0.0",
    "chalk": "^4.0.0",
    "faker": "^4.1.0",
    "jest": "^25.3.0",
    "livereload": "^0.9.1",
    "prettier": "^2.0.4",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
}
