    "@types/node": "^16.10.1",
    "@types/node-fetch": "^2.5.12",
    "@types/nodemailer": "^6.4.4",
    "@types/prettier": "^2.4.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/redis": "^2.8.32",
    "@types/request-ip": "^0.0.37",
    "@types/ua-parser-js": "^0.7.36",
    "@types/yargs": "^17.0.3",
    "ansi-to-html": "^0.7.1",
    "babel-loader": "^8.2.2",
    "camelcase": "^6.2.0",
    "concurrently": "^6.2.1",
    "cors": "^2.8.5",
    "escape-html": "^1.0.3",
    "faker": "^5.5.3",
    "jest": "^27.2.2",
    "livereload": "^0.9.3",
    "localtunnel": "^2.0.2",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  }
}
