    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.6.0",
    "graphql-iso-date": "^3.6.1",
    "inversify": "^5.0.1",
    "jwt-decode": "^2.2.0",
    "knex": "^0.20.9",
    "nexus": "^0.12.0-rc.9",
    "node-fetch": "^2.6.0",
    "pg": "^7.18.1",
