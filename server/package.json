{
  "name": "@parallel/server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --ignore-watch graphql/__types.ts --no-notify --transpile-only --respawn ./src",
    "debug": "cross-env NODE_ENV=development ts-node-dev --inspect --ignore-watch graphql/__types.ts --no-notify --transpile-only --respawn ./src",
    "nodemon": "cross-env NODE_ENV=development nodemon --watch 'src/' --ignore 'src/graphql/__types.ts' -e 'ts tsx json' --exec 'node -r ts-node/register/transpile-only' --inspect src/index.ts",
    "build": "tsc --project tsconfig.prod.json",
    "prebuild": "yarn generate-i18n-files",
    "migrate": "knex migrate:latest",
    "postmigrate": "yarn generate-db-types",
    "migrate-make": "knex migrate:make",
    "migrate-dryrun": "knex migrate:list",
    "migrate-down": "knex migrate:down",
    "postmigrate-down": "yarn generate-db-types",
    "seed": "knex seed:run",
    "generate-db-types": "ts-node ./bin/generate-db-types.ts",
    "test": "jest --runInBand",
    "extract-i18n-terms": "yarn workspace @parallel/bin extract-i18n-terms --locales en es --input '../server/src/emails/{components,common}/**/*.{ts,tsx}' --output '../server/lang'",
    "generate-i18n-files": "yarn workspace @parallel/bin generate-i18n-files --locales en es --input '../server/lang' --output-compiled '../server/lang/compiled'",
    "dev-workers": "yarn concurrently \"yarn:worker:* start\"",
    "worker:email-events": "yarn ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/email-events.ts",
    "worker:email-sender": "yarn ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/email-sender.ts",
    "worker:event-processor": "yarn ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/event-processor.ts",
    "worker:reminder-trigger": "yarn ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/reminder-trigger.ts",
    "worker:scheduled-trigger": "yarn ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/scheduled-trigger.ts",
    "worker:signature": "yarn ts-node-dev --project tsconfig.worker.json --no-notify --transpile-only --respawn src/workers/signature-worker.ts",
    "localtunnel": "ts-node ./bin/launch-localtunnel.ts",
    "emails": "concurrently \"ts-node-dev --watch ./emails.tsx --ignore-watch ./src --files ./emails.tsx --respawn\" \"start-storybook --port 60000 --quiet\"",
    "postinstall": "cd .. && yarn patch-package",
    "generate-graphql-types": "graphql-codegen",
    "run-ts-script": "ts-node --project tsconfig.json --transpile-only"
  },
  "dependencies": {
    "@nexus/schema": "^0.20.1",
    "ajv": "^8.2.0",
    "ajv-formats": "^2.0.2",
    "amazon-cognito-identity-js": "^4.6.0",
    "analytics-node": "^4.0.1",
