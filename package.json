{
  "license": "UNLICENSED",
  "private": true,
  "workspaces": [
    "bin",
    "client",
    "server",
    "e2e"
  ],
  "scripts": {
    "lint": "eslint --config .eslintrc.js .",
    "update-deps": "yarn ncux && yarn ncux --packageFile bin/package.json && yarn ncux --packageFile client/package.json && yarn ncux --packageFile server/package.json  && yarn ncux --packageFile e2e/package.json",
    "ud": "yarn update-deps",
    "ncux": "ncu -u -x 'chalk,camelcase,escape-string-regexp,p-all,p-map,p-props,node-fetch,@types/node-fetch,file-type,graphql-upload,@types/graphql-upload,email-providers'",
    "postinstall": "patch-package",
    "postpull": "yarn && cd server && yarn migrate"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@typescript/analyze-trace": "^0.10.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-formatjs": "^4.10.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "7.33.2",
    "npm-check-updates": "^16.12.2",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.0.2"
  },
  "dependencies": {
    "patch-package": "^8.0.0"
  },
  "resolutions": {
    "@apollo/client": "^3.8.1",
    "@emotion/react": "^11.11.1",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6",
    "@types/react": "^18.2.20",
    "@types/express": "^4.17.17"
  }
}
