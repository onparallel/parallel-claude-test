{
  "license": "UNLICENSED",
  "private": true,
  "workspaces": [
    "bin",
    "client",
    "server",
    "e2e"
  ],
  "scripts": {
    "lint": "eslint --quiet --config .eslintrc.js .",
    "lint-warnings": "eslint --config .eslintrc.js .",
    "update-deps": "yarn ncux && yarn ncux --packageFile bin/package.json && yarn ncux --packageFile client/package.json && yarn ncux --packageFile server/package.json  && yarn ncux --packageFile e2e/package.json",
    "ud": "yarn update-deps",
    "ncux": "ncu -u -x 'chalk,camelcase,escape-string-regexp,p-all,p-map,p-props,node-fetch,@types/node-fetch,file-type,graphql-upload,graphql-request,@types/graphql-upload,graphql-upload-client,flat'",
    "postinstall": "patch-package",
    "postpull": "yarn && cd server && yarn migrate"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@typescript/analyze-trace": "^0.10.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-formatjs": "^4.13.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "7.35.0",
    "npm-check-updates": "^17.1.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "patch-package": "^8.0.0"
  },
  "resolutions": {
    "graphql": "^16.9.0",
    "@apollo/client": "^3.11.4",
    "@emotion/react": "^11.11.3",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "@types/react": "^18.3.4",
    "@types/express": "^4.17.21",
    "@typescript-eslint/typescript-estree": "^7.10.0",
    "postgres-interval": "^4.0.2"
  }
}