{
  "license": "UNLICENSED",
  "private": true,
  "workspaces": [
    "bin",
    "client",
    "server",
    "e2e"
  ],
  "scripts": {
    "lint": "eslint --config .eslintrc.js .",
    "update-deps": "yarn ncux && yarn ncux --packageFile bin/package.json && yarn ncux --packageFile client/package.json && yarn ncux --packageFile server/package.json  && yarn ncux --packageFile e2e/package.json",
    "ncux": "ncu -u -x 'chalk,camelcase,escape-string-regexp,p-all,p-map,p-props,node-fetch,@types/node-fetch,file-type,graphql-upload,@types/graphql-upload,email-providers'",
    "postinstall": "patch-package",
    "postpull": "yarn && cd server && yarn migrate"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@typescript/analyze-trace": "^0.10.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-formatjs": "^4.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.32.2",
    "npm-check-updates": "^16.10.8",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.8.7"
  },
  "dependencies": {
    "patch-package": "^6.5.1"
  },
  "resolutions": {
    "core-js": "^3.27.2",
    "csstype": "^3.0.9",
    "typescript": "^4.9.4",
    "@types/react": "^18.0.37",
    "@types/express": "^4.17.16"
  }
}
