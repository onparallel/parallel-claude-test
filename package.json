{
  "license": "UNLICENSED",
  "private": true,
  "workspaces": [
    "bin",
    "client",
    "server",
    "e2e"
  ],
  "scripts": {
    "lint": "eslint --quiet .",
    "lint-warnings": "eslint .",
    "update-deps": "yarn ncux && yarn ncux --packageFile bin/package.json && yarn ncux --packageFile client/package.json && yarn ncux --packageFile server/package.json  && yarn ncux --packageFile e2e/package.json",
    "ud": "yarn update-deps",
    "ncux": "ncu -u -x 'chalk,camelcase,escape-string-regexp,p-all,p-map,p-props,file-type,graphql-upload,graphql-request,@types/graphql-upload,graphql-upload-client,flat,@react-hook/resize-observer'",
    "cp-scripts": "cp node_modules/pdfjs-dist/legacy/build/pdf.worker.min.mjs node_modules/@onparallel/consent-manager-do-not-use/standalone/consent-manager.js node_modules/redoc/bundles/redoc.standalone.js client/public/static/js/",
    "update-ip-db": "if [ -z \"$SKIP_UPDATE_IP_DB\" ]; then node node_modules/ip-location-api/script/updatedb.mjs; fi",
    "postinstall": "patch-package && yarn cp-scripts && yarn update-ip-db",
    "postpull": "yarn && cd server && yarn migrate"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@typescript/analyze-trace": "^0.10.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-formatjs": "^4.13.3",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "7.37.5",
    "globals": "^17.3.0",
    "npm-check-updates": "^17.1.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "patch-package": "^8.0.0"
  },
  "resolutions": {
    "graphql": "^16.9.0",
    "@apollo/client": "^4.0.7",
    "@emotion/react": "^11.11.3",
    "express": "^4.22.1",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "@types/react": "^18.3.4",
    "@types/express": "^4.17.21",
    "postgres-interval": "^4.0.2",
    "tmp": "0.2.4",
    "nodemailer": "7.0.11"
  }
}
