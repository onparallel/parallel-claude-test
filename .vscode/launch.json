{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach",
      "port": 9229,
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "name": "Next.js: debug server-side",
      "type": "node-terminal",
      "request": "launch",
      "command": "yarn workspace @parallel/client dev"
    },
    {
      "name": "Sever",
      "type": "node-terminal",
      "request": "launch",
      "command": "yarn workspace @parallel/server debug"
    },
    {
      "name": "Storybook",
      "type": "node-terminal",
      "request": "launch",
      "command": "yarn workspace @parallel/server _stories"
    }
  ]
}
